    405          /*********************************************************************
    406           * @fn      GenericApp_HandleKeys
    407           *
    408           * @brief   Handles all key events for this device.
    409           *
    410           * @param   shift - true if in shift/alt.
    411           * @param   keys - bit field for key events. Valid entries:
    412           *                 HAL_KEY_SW_4
    413           *                 HAL_KEY_SW_3
    414           *                 HAL_KEY_SW_2
    415           *                 HAL_KEY_SW_1
    416           *
    417           * @return  none
    418           */
    419          static void GenericApp_HandleKeys( uint8 shift, uint8 keys )
    420          {
    421            zAddrType_t dstAddr;
    422          
    423            // Shift is used to make each button/switch dual purpose.
    424            if ( shift )
    425            {
    426              if ( keys & HAL_KEY_SW_1 )
    427              {
    428              }
    429              if ( keys & HAL_KEY_SW_2 )
    430              {
    431              }
    432              if ( keys & HAL_KEY_SW_3 )
    433              {
    434              }
    435              if ( keys & HAL_KEY_SW_4 )
    436              {
    437              }
    438            }
    439            else
    440            {
    441              if ( keys & HAL_KEY_SW_1 )
    442              {
    443                // Since SW1 isn't used for anything else in this application...
    444          #if defined( SWITCH1_BIND )
    445                // we can use SW1 to simulate SW2 for devices that only have one switch,
    446                keys |= HAL_KEY_SW_2;
    447          #elif defined( SWITCH1_MATCH )
    448                // or use SW1 to simulate SW4 for devices that only have one switch
    449                keys |= HAL_KEY_SW_4;
    450          #endif
    451              }
    452          
    453              if ( keys & HAL_KEY_SW_2 )
    454              {
    455                HalLedSet ( HAL_LED_4, HAL_LED_MODE_OFF );
    456          
    457                // Initiate an End Device Bind Request for the mandatory endpoint
    458                dstAddr.addrMode = Addr16Bit;
    459                dstAddr.addr.shortAddr = 0x0000; // Coordinator
    460                ZDP_EndDeviceBindReq( &dstAddr, NLME_GetShortAddr(),
    461                                      GenericApp_epDesc.endPoint,
    462                                      GENERICAPP_PROFID,
    463                                      GENERICAPP_MAX_CLUSTERS, (cId_t *)GenericApp_ClusterList,
    464                                      GENERICAPP_MAX_CLUSTERS, (cId_t *)GenericApp_ClusterList,
    465                                      FALSE );
    466              }
    467          
    468              if ( keys & HAL_KEY_SW_3 )
    469              {
    470              }
    471          
    472              if ( keys & HAL_KEY_SW_4 )
    473              {
    474                HalLedSet ( HAL_LED_4, HAL_LED_MODE_OFF );
    475                // Initiate a Match Description Request (Service Discovery)
    476                dstAddr.addrMode = AddrBroadcast;
    477                dstAddr.addr.shortAddr = NWK_BROADCAST_SHORTADDR;
    478                ZDP_MatchDescReq( &dstAddr, NWK_BROADCAST_SHORTADDR,
    479                                  GENERICAPP_PROFID,
    480                                  GENERICAPP_MAX_CLUSTERS, (cId_t *)GenericApp_ClusterList,
    481                                  GENERICAPP_MAX_CLUSTERS, (cId_t *)GenericApp_ClusterList,
    482                                  FALSE );
    483              }
    484            }
    485          }
