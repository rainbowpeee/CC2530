    542          /*********************************************************************
    543           * @fn      GenericApp_ProcessRtosMessage
    544           *
    545           * @brief   Receive message from RTOS queue, send response back.
    546           *
    547           * @param   none
    548           *
    549           * @return  none
    550           */
    551          static void GenericApp_ProcessRtosMessage( void )
    552          {
    553            osalQueue_t inMsg;
    554          
    555            if ( osal_queue_receive( OsalQueue, &inMsg, 0 ) == pdPASS )
    556            {
    557              uint8 cmndId = inMsg.cmnd;
    558              uint32 counter = osal_build_uint32( inMsg.cbuf, 4 );
    559          
    560              switch ( cmndId )
    561              {
    562                case CMD_INCR:
    563                  counter += 1;  /* Increment the incoming counter */
    564                                 /* Intentionally fall through next case */
    565          
    566                case CMD_ECHO:
    567                {
    568                  userQueue_t outMsg;
    569          
    570                  outMsg.resp = RSP_CODE | cmndId;  /* Response ID */
    571                  osal_buffer_uint32( outMsg.rbuf, counter );    /* Increment counter */
    572                  osal_queue_send( UserQueue1, &outMsg, 0 );  /* Send back to UserTask */
    573                  break;
    574                }
    575                
    576                default:
    577                  break;  /* Ignore unknown command */    
    578              }
    579            }
    580          }
    581          #endif
